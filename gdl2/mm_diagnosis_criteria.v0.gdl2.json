    "default_actions": [
      "$gt0033|Evidence of end organ damage|=0|local::at0049|Absent|",
      "$gt0034|Biomarkers of malignancy|=0|local::at0051|Absent|",
      "$gt0028|Plasmacytoma|=0|local::at0039|Absent|",
      "$gt0035|Multiple Myeloma diagnosis|=0|local::at0053|Absent|",
      "$gt0030|Hypercalcaemia|=0|local::at0043|Absent|",
      "$gt0029|Renal Insufficiency|=0|local::at0041|Absent|",
      "$gt0032|Bone lesions|=0|local::at0047|False|"
    ],
    "rules": {
      "gt0036": {
        "id": "gt0036",
        "priority": 13,
        "when": [
          "$gt0004|clonal BM plasma cells result|>=10,%"
        ],
        "then": [
          "$gt0027|Clonal BM plasma cells %|=1|local::at0038|>= 10%|",
          "$gt0108|Clonal BM plasma cells|=local::at0028|>=10%|"
        ]
      },
      "gt0072": {
        "id": "gt0072",
        "priority": 12,
        "when": [
          "($gt0071|Diagnosis of Plasmacytoma|==local::at0057|Extramedullary plasmacytoma|)||($gt0071|Diagnosis of Plasmacytoma|==local::at0056|Biopsy-proven bony plasmacytoma|)"
        ],
        "then": [
          "$gt0028|Plasmacytoma|=1|local::at0040|Present|"
        ]
      },
      "gt0046": {
        "id": "gt0046",
        "priority": 11,
        "when": [
          "($gt0042|Haemoglobin result|<100,gm/l)||($gt0042|Haemoglobin result|<10,gm/dl)"
        ],
        "then": [
          "$gt0031|Anaemia|=1|local::at0046|Present|",
          "$gt0106|Haemoglobin|=local::at0021|< 100 g/l|"
        ]
      },
      "gt0050": {
        "id": "gt0050",
        "priority": 10,
        "when": [
          "$gt0052|Calcium result|>2.75,mmol/l"
        ],
        "then": [
          "$gt0030|Hypercalcaemia|=1|local::at0044|Present|",
          "$gt0103|Calcium|=local::at0012|> 2.75 mmol/l|"
        ]
      },
      "gt0056": {
        "id": "gt0056",
        "priority": 9,
        "when": [
          "$gt0047|CrCl result|<40,ml/min"
        ],
        "then": [
          "$gt0029|Renal Insufficiency|=1|local::at0042|Present|",
          "$gt0104|CrCl|=local::at0015|< 40 ml/min|"
        ]
      },
      "gt0111": {
        "id": "gt0111",
        "priority": 8,
        "when": [
          "($gt0053|Creatinine result|>177,umol/l)||($gt0053|Creatinine result|>2,mg/dl)"
        ],
        "then": [
          "$gt0029|Renal Insufficiency|=1|local::at0042|Present|",
          "$gt0105|Creatinine|=local::at0018|> 177 umol/l|"
        ]
      },
      "gt0057": {
        "id": "gt0057",
        "priority": 7,
        "when": [
          "$gt0098|Osteolytic lesions|>=1"
        ],
        "then": [
          "$gt0032|Bone lesions|=1|local::at0048|True|",
          "$gt0107|Osteolytic lesions|=local::at0025|>=1|"
        ]
      },
      "gt0075": {
        "id": "gt0075",
        "priority": 6,
        "when": [
          "($gt0030|Hypercalcaemia|==1|local::at0044|Present|)||(($gt0029|Renal Insufficiency|==1|local::at0042|Present|)||(($gt0032|Bone lesions|==1|local::at0048|True|)||($gt0031|Anaemia|==1|local::at0046|Present|)))"
        ],
        "then": [
          "$gt0033|Evidence of end organ damage|=1|local::at0050|Present|"
        ]
      },
      "gt0081": {
        "id": "gt0081",
        "priority": 5,
        "when": [
          "$gt0004|clonal BM plasma cells result|>=60,%"
        ],
        "then": [
          "$gt0027|Clonal BM plasma cells %|=2|local::at0070|>= 60%|",
          "$gt0108|Clonal BM plasma cells|=local::at0029|>= 60%|"
        ]
      },
      "gt0095": {
        "id": "gt0095",
        "priority": 4,
        "when": [
          "$gt0097|FLC ratio|>=100"
        ],
        "then": [
          "$gt0090|Serum FLC ratio >= 100|=1|local::at0074|True|",
          "$gt0109|Involved/uninvolved serum-FLC ratio|=local::at0032|>= 100|"
        ]
      },
      "gt0092": {
        "id": "gt0092",
        "priority": 3,
        "when": [
          "$gt0101|FLs >= 5mm|>=1"
        ],
        "then": [
          "$gt0089|Focal lesion on MRI studies >=1|=1|local::at0077|True|",
          "$gt0110|Focal lesions on MRI studies (size >= 5mm)|=local::at0035|>=1|"
        ]
      },
      "gt0085": {
        "id": "gt0085",
        "priority": 2,
        "when": [
          "($gt0027|Clonal BM plasma cells %|==2|local::at0070|>= 60%|)||(($gt0089|Focal lesion on MRI studies >=1|==1|local::at0077|True|)||($gt0090|Serum FLC ratio >= 100|==1|local::at0074|True|))"
        ],
        "then": [
          "$gt0034|Biomarkers of malignancy|=1|local::at0052|Present|"
        ]
      },
      "gt0084": {
        "id": "gt0084",
        "priority": 1,
        "when": [
          "($gt0027|Clonal BM plasma cells %|==1|local::at0038|>= 10%|)||($gt0028|Plasmacytoma|==1|local::at0040|Present|)",
          "$gt0033|Evidence of end organ damage|==1|local::at0050|Present|",
          "$gt0034|Biomarkers of malignancy|==1|local::at0052|Present|",
          "($gt0033|Evidence of end organ damage|==1|local::at0050|Present|)||($gt0034|Biomarkers of malignancy|==1|local::at0052|Present|)"
        ],
        "then": [
          "$gt0035|Multiple Myeloma diagnosis|=1|local::at0054|Present|"
        ]
      }
    }
